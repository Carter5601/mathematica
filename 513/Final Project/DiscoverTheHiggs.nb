      RowBox[{"NExpAfterCuts", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"NExpBeforeCuts", "*", "#"}], "&"}], "/@", "efficiency"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"ExpError", "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          FractionBox["NExpBeforeCuts", 
           RowBox[{"cutlist", "[", 
            RowBox[{"[", "1", "]"}], "]"}]], "*", 
          RowBox[{"PoissonError", "[", "#", "]"}]}], "&"}], "/@", 
        "cutlist"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"ExpCutsWithError", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"Length", "[", "cutlist", "]"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "<", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Length", "[", "ExpCutsWithError", "]"}], "+", "1"}], 
          ")"}]}], ",", 
