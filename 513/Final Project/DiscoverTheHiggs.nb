Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"LeptonGaussSmear", "[", "event_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "OldleptonEvent", ",", "NewLeptonEvent", ",", "lepE", ",", 
       "lep\[Sigma]", ",", "newE", ",", "newPvec", ",", "tempEvent", ",", 
       "i"}], "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"OldleptonEvent", "=", 
       RowBox[{"GetParticles", "[", 
        RowBox[{"event", ",", "PIDLeptonList"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"NewLeptonEvent", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "<", 
         RowBox[{
          RowBox[{"Length", "[", "OldleptonEvent", "]"}], "+", "1"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"tempEvent", "=", 
          RowBox[{"OldleptonEvent", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", 
         RowBox[{"lepE", "=", 
          RowBox[{"enOf", "[", "tempEvent", "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"lep\[Sigma]", "=", 
          RowBox[{"LepERes", "[", "lepE", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"newE", "=", 
          RowBox[{"RandomVariate", "[", 
           RowBox[{"NormalDistribution", "[", 
            RowBox[{"lepE", ",", "lep\[Sigma]"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"newPvec", "=", 
          RowBox[{
           FractionBox["newE", "lepE"], " ", 
           RowBox[{"ThreeVectorFrom", "[", "tempEvent", "]"}]}]}], ";", " ", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"tempEvent", "[", 
           RowBox[{"[", "10", "]"}], "]"}], "=", "newE"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"tempEvent", "[", 
           RowBox[{"[", 
            RowBox[{"7", ";;", "9"}], "]"}], "]"}], "=", "newPvec"}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"NewLeptonEvent", ",", "tempEvent"}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", "NewLeptonEvent"}]}], "\[IndentingNewLine]", 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.858254608401897*^9, 3.858254613058322*^9}},
 CellLabel->
  "In[263]:=",ExpressionUUID->"3d569d80-6bf9-4d2e-ae36-1d09266495e9"],

