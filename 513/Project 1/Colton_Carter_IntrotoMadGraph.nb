Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"rapOf", "[", 
    RowBox[{"{", 
     RowBox[{
     "_", ",", "_", ",", "_", ",", "_", ",", "_", ",", "_", ",", "px_", ",", 
      "py_", ",", "pz_", ",", "e_", ",", "___"}], "}"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", "2"], "*", 
    RowBox[{"Log", "[", 
     FractionBox[
      RowBox[{"(", 
       RowBox[{"e", "+", "pz"}], ")"}], 
      RowBox[{"e", "-", "pz"}]], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"etaOf", "[", 
    RowBox[{"{", 
     RowBox[{
     "_", ",", "_", ",", "_", ",", "_", ",", "_", ",", "_", ",", "px_", ",", 
      "py_", ",", "pz_", ",", "e_", ",", "___"}], "}"}], "]"}], ":=", 
   RowBox[{"-", 
    RowBox[{"Log", "[", 
     RowBox[{"Tan", "[", 
      FractionBox[
       RowBox[{"ArcCos", "[", 
        FractionBox["pz", 
         SqrtBox[
          RowBox[{
           SuperscriptBox["px", "2"], " ", "+", " ", 
           SuperscriptBox["py", "2"], "+", " ", 
           SuperscriptBox["pz", "2"]}]]], "]"}], "2"], "]"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"rap", "[", "event_", "]"}], " ", ":=", 
