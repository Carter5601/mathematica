Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"AbsMin", "[", "list_", "]"}], ":=", 
   RowBox[{
    RowBox[{"Sort", "[", 
     RowBox[{"list", ",", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", "#1", "]"}], "<", 
        RowBox[{"Abs", "[", "#2", "]"}]}], "&"}]}], "]"}], 
    "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DeltaPhi", "[", 
    RowBox[{"{", 
     RowBox[{"part1_", ",", "part2_"}], "}"}], "]"}], ":=", 
   RowBox[{"AbsMin", "[", 
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"phiOf", "[", "part1", "]"}], "-", 
        RowBox[{"phiOf", "[", "part2", "]"}]}], ")"}], "\[IndentingNewLine]", 
      ",", "\[IndentingNewLine]", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", " ", "\[Pi]"}], " ", "-", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"phiOf", "[", "part1", "]"}], "-", 
          RowBox[{"phiOf", "[", "part2", "]"}]}], ")"}]}], ")"}], 
      "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", " ", "\[Pi]"}], " ", "+", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"phiOf", "[", "part1", "]"}], "-", 
          RowBox[{"phiOf", "[", "part2", "]"}]}], ")"}]}], ")"}]}], 
     "\[IndentingNewLine]", "}"}], "]"}]}], ";"}]}], "Input",
