Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"MCtoExpCutlist", "[", 
    RowBox[{"cutlist_", ",", "NExpBeforeCuts_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "efficiency", ",", "NExpAfterCuts", ",", "ExpError", ",", 
       "ExpCutsWithError"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"efficiency", "=", 
       RowBox[{
        RowBox[{
         FractionBox["#", 
          RowBox[{"cutlist", "[", 
           RowBox[{"[", "1", "]"}], "]"}]], "&"}], "/@", " ", "cutlist"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"NExpAfterCuts", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"NExpBeforeCuts", "*", "#"}], "&"}], "/@", "efficiency"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"ExpError", "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          FractionBox["NExpBeforeCuts", 
           RowBox[{"cutlist", "[", 
            RowBox[{"[", "1", "]"}], "]"}]], "*", 
          RowBox[{"PoissonError", "[", "#", "]"}]}], "&"}], "/@", 
        "cutlist"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"ExpCutsWithError", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"Length", "[", "cutlist", "]"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "<", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Length", "[", "ExpCutsWithError", "]"}], "+", "1"}], 
          ")"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"ExpCutsWithError", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "=", 
          RowBox[{"Subsuperscript", "[", 
           RowBox[{
            RowBox[{"ToString", "[", 
             RowBox[{"N", "[", 
              RowBox[{"NExpAfterCuts", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "]"}], "]"}], ",", 
            RowBox[{"ToString", "[", 
             RowBox[{"ExpError", "[", 
              RowBox[{"[", 
               RowBox[{"i", ",", "1"}], "]"}], "]"}], "]"}], ",", 
            RowBox[{"ToString", "[", 
             RowBox[{"ExpError", "[", 
              RowBox[{"[", 
               RowBox[{"i", ",", "2"}], "]"}], "]"}], "]"}]}], "]"}]}], 
         ";"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"ExpCutsWithError", ",", "NExpAfterCuts"}], "}"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.627904588463702*^9, 3.6279046420707684`*^9}, {
