Cell[BoxData[
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "Vud", ",", "Vus", ",", "Vcd", ",", "Vcs", ",", "Vuddag", ",", "Vcddag", 
    ",", "Vusdag", ",", "Vcsdag"}], "]"}], ";"}]], "Input",
