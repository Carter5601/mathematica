Cell[BoxData[{
 RowBox[{
  RowBox[{"Vud", "=", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{"I", "*", "alpha"}], "]"}], "*", 
    RowBox[{"Cos", "[", "theta", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Vus", "=", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"Exp", "[", 
      RowBox[{"I", "*", 
       RowBox[{"(", 
        RowBox[{"alpha", "+", "gamma"}], ")"}]}], "]"}]}], "*", 
    RowBox[{"Sin", "[", "theta", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Vcd", "=", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{"I", "*", "beta"}], "]"}], "*", 
    RowBox[{"Sin", "[", "theta", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Vcs", "=", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{"I", "*", 
      RowBox[{"(", 
       RowBox[{"beta", "+", "gamma"}], ")"}]}], "]"}], "*", 
    RowBox[{"Cos", "[", "theta", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Vuddag", "=", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "I"}], "*", "alpha"}], "]"}], "*", 
    RowBox[{"Cos", "[", "theta", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Vusdag", "=", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "I"}], "*", 
       RowBox[{"(", 
        RowBox[{"alpha", "+", "gamma"}], ")"}]}], "]"}]}], "*", 
    RowBox[{"Sin", "[", "theta", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Vcddag", "=", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "I"}], "*", "beta"}], "]"}], "*", 
    RowBox[{"Sin", "[", "theta", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Vcsdag", "=", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "I"}], "*", 
      RowBox[{"(", 
       RowBox[{"beta", "+", "gamma"}], ")"}]}], "]"}], "*", 
    RowBox[{"Cos", "[", "theta", "]"}]}]}], ";"}]}], "Input",
